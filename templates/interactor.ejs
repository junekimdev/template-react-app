import { Dispatch, SetStateAction } from 'react';

export const useLinkClick = (setState: Dispatch<SetStateAction<string>>) => {
  return useCallback(
    (e: MouseEvent<HTMLAnchorElement>) => {
      const id: string = e.target.id ?? '';
      setState(id);
    },
    [setState],
  );
};
